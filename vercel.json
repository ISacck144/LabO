{
  "version": 2,
  "builds": [
    {
      "src": "MyDjangoProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "includeFiles": [
          "requirements.txt",
          "vercel_build.sh"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "MyDjangoProject/wsgi.py"
    }
  ]
}
